{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "static/**",
        "use": "@vercel/static"
    } 
    ],
    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.+\\.[a-z]+)$",
        "dest": "static/$1"
    } 
    ]
  }