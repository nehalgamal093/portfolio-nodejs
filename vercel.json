{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/public"
    } 
    ],
    
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js"
      },
      {
        "src": "/(.+)$",
        "dest": "public/$1"
    } 
    ]
  }